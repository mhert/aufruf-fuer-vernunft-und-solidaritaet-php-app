{
  "name": "mhert/aufruf-fuer-vernunft-und-solidaritaet",
  "description": "",
  "license": "proprietary",
  "type": "project",
  "require": {
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/annotations": "^1.13.2",
    "predis/predis": "^1.1",
    "ramsey/uuid": "^4.2.3",
    "sensio/framework-extra-bundle": "^6.2.4",
    "symfony/asset": "^6.0.0",
    "symfony/config": "^6.0.2",
    "symfony/console": "^6.0.2",
    "symfony/dependency-injection": "^6.0.2",
    "symfony/error-handler": "^6.0.2",
    "symfony/flex": "^2.0.1",
    "symfony/form": "^6.0.0",
    "symfony/framework-bundle": "^6.0.2",
    "symfony/http-kernel": "^6.0.0",
    "symfony/routing": "^6.0.1",
    "symfony/runtime": "^6.0.0",
    "symfony/security-csrf": "^6.0.0",
    "symfony/twig-bundle": "^6.0.0",
    "symfony/yaml": "^6.0.2",
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.22.0",
    "phpunit/phpunit": "^9.5.11"
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php56": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "autoload": {
    "psr-4": {
      "Mhert\\AufrufFuerVernunftUndSolidaritaet\\App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mhert\\AufrufFuerVernunftUndSolidaritaet\\App\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "infection/extension-installer": false
    },
    "platform": {
      "php": "8.1.1"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^6.0.0"
    }
  }
}
